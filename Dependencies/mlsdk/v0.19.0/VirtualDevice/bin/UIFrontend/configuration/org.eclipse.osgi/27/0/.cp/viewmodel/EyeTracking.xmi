<?xml version="1.0" encoding="UTF-8"?>
<viewmodel:View
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:viewmodel="com.magicleap.uidesigner.rcp.properties.viewmodel"
    xsi:schemaLocation="com.magicleap.uidesigner.rcp.properties.viewmodel ../../com.magicleap.uidesigner.properties.model/model/viewmodel.ecore"
    viewId="vdf.eyetracking"
    typeId="EyeTracking">
  <children
      xsi:type="viewmodel:VerticalLayout"
      viewId="vdf.eyetracking.container">
    <children
        xsi:type="viewmodel:Group"
        viewId="vdf.eyetracking.eyes"
        label="Eyes">
      <children
          xsi:type="viewmodel:Control"
          viewId="vdf.eyetracking.eyes.fixation"
          id="fixation"/>
      <children
          xsi:type="viewmodel:Control"
          viewId="vdf.eyetracking.eyes.left_eye"
          id="left_eye"/>
      <children
          xsi:type="viewmodel:Control"
          viewId="vdf.eyetracking.eyes.right_eye"
          id="right_eye"/>
    </children>
    <children
        xsi:type="viewmodel:Group"
        viewId="vdf.eyetracking.state"
        label="State">
      <children
          xsi:type="viewmodel:Control"
          viewId="vdf.eyetracking.state.state"
          id="state"
          labelAlignment="None"/>
    </children>
  </children>
</viewmodel:View>
